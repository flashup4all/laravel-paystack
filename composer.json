{
    "name": "phemorah/laravel-paystack",
    "description": "A Laravel 6 Package for Paystack",
    "keywords": ["php","github", "laravel","Open Source","payments", "subscription", "paystack", "paystack.co","laravel 6"],
    "license": "MIT",
    "authors": [
      {
        "name": "phemorah",
        "email": "phemorah@gmail.com"
      }
   ],
   "minimum-stability": "dev",
   "require": {
    "php": "^5.4.0|^7.0|^7.2",
    "illuminate/support": "5.*|6.*",
    "guzzlehttp/guzzle": "5.*|6.*"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*|7.2",
        "scrutinizer/ocular": "~1.1",
        "satooshi/php-coveralls": "^0.7.0",
        "mockery/mockery": ">=0.7.2"
    },
   "autoload": {
        "psr-4": {
          "Phemorah\\Paystack\\": "src"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Phemorah\\Paystack\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
   "extra": {
    "laravel": {
        "providers": [
            "Phemorah\\Paystack\\PaystackServiceProvider"
        ],
        "aliases": [
            "Phemorah\\Paystack\\Facades\\Paystack"
        ]
    }
  }
}
